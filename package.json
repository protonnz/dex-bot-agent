{
  "name": "dexbot",
  "version": "1.0.0",
  "description": "trading bot against proton dex",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "bot": "tsc --noEmit && node --no-warnings=ExperimentalWarning --loader ts-node/esm ./src/index.ts",
    "bot:test": "cross-env NODE_ENV=test npm run bot",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix --ext .js",
    "test": "mocha"
  },
  "keywords": [
    "proton",
    "dex",
    "trade",
    "bot",
    "order",
    "crypto"
  ],
  "author": "Pravin Battu",
  "license": "MIT",
  "dependencies": {
    "@proton/js": "^27.13.0",
    "@proton/light-api": "^3.3.3",
    "@proton/wrap-constants": "^0.2.68",
    "@slack/bolt": "^3.12.2",
    "@slack/socket-mode": "^1.3.2",
    "@slack/web-api": "^6.8.1",
    "@types/winston": "^2.4.4",
    "bignumber.js": "^9.1.2",
    "config": "^3.3.8",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.0.0",
    "readline": "^1.3.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/chai": "^4.3.4",
    "@types/config": "^3.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.6.11",
    "@types/sqlite3": "^3.1.11",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "globals": "^15.11.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.9.0"
  }
